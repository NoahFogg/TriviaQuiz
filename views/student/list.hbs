this displays list of students as a <table></table>
<h3><a class="btn" href= "/student">Create New</a>Student List </h3>
<table class = "table">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.Mobile}}</td>
            <td>{{this.city}}</td>
            <td>
                <a class="btn" href = "/student/{{this.id}}">Edit</a>
                <a class="btn" href = "/student/delete/{{this.id}}"
                onclick="return confirm('You sure?');">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>